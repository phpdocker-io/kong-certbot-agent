{
  "name": "phpdockerio/kong-certbot",
  "autoload": {
    "psr-4": {
      "PhpDockerIo\\KongCertbot\\": "src/",
      "Tests\\PhpDockerIo\\KongCertbot\\": "tests/"
    }
  },
  "license": "MIT",
  "require":  {
    "php":               "^8.0",
    "ext-json":          "*",
    "ext-dom":           "*",
    "ext-curl":          "*",
    "guzzlehttp/guzzle": "^7.3",
    "symfony/console":   "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "infection/infection": "^0.25.0",
    "symfony/var-dumper": "^5.0",
    "phpstan/phpstan": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
